{
  "name": "mocking-birds",
  "version": "1.0.0",
  "description": "expressjs server that gathers all the lcfe mock servers",
  "main": "dist/index.js",
  "bin": {
    "mocking-bird": "bin/mocking-bird.js"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && ./node_modules/.bin/babel --presets es2015 -d ./dist/ ./src/",
    "start": "npm run clean && ./node_modules/.bin/babel --presets es2015 -w -d ./dist/ ./src/",
    "clean": "rm -rf dist && mkdir dist",
    "test": "mocha ./test/**/*Spec.js",
    "lint": "./node_modules/.bin/eslint ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goncalvesjoao/mocking-birds.git"
  },
  "author": "croudcare",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goncalvesjoao/mocking-birds/issues"
  },
  "homepage": "https://github.com/goncalvesjoao/mocking-birds#readme",
  "dependencies": {
    "body-parser": "^1.14.1",
    "commander": "^2.9.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "fs-extra": "^0.26.2"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.9.0"
  }
}
